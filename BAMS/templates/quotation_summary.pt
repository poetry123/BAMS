<div metal:use-macro="load: layout.pt">
  <div metal:fill-slot="content">
      <div class="x_panel">
                  <div class="x_title">
                    <h2>Quotation Summary</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table id="datatable-buttons" class="table table-striped table-bordered" style="font-size:13px">
                      <thead>
                        <tr>
                          <th>Name</th>
                          <th>Position</th>
                          <th>Office</th>
                          <th>Age</th>
                          <th>Start date</th>
                          <th>Salary</th>
                        </tr>
                      </thead>

                      <tbody>
                        <tr tal:repeat="quotation quotations">
                        <td>${quotation.name}</td>
                        <td>${quotation.position}</td>
                        <td>${quotation.office}</td>
                        <td>${quotation.age}</td>
                        <td>${quotation.start_date}</td>
                        <td>$ ${quotation.salary}</td>
                      </tr>
                      </tbody>
                    </table>
                  </div>
                </div>
  </div>
</div>
